<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- Return -->
        <record id="seq_type_stock_vendor_lot" model="ir.sequence.type">
            <field name="name">Vendor Lot</field>
            <field name="code">stock.vendor.lot</field>
        </record>

        <record id="seq_stock_vendor_lot" model="ir.sequence">
            <field name="name">Vendor Lots</field>
            <field name="code">stock.vendor.lot</field>
            <field name="prefix">%(y)s-%(month)s-%(day)s-</field>
            <field name="padding">0</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_type_stock_vendor_receipt" model="ir.sequence.type">
            <field name="name">Vendor Receipt</field>
            <field name="code">stock.vendor.receipt</field>
        </record>

        <record id="seq_stock_vendor_receipt" model="ir.sequence">
            <field name="name">Vendor Receipts</field>
            <field name="code">stock.vendor.receipt</field>
            <field name="prefix">IN-%(y)s-%(month)s-%(day)s-</field>
            <field name="padding">0</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
